{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": "warn",
    "eol-last": "off",
    "max-len": ["error", 150, { "ignoreUrls": true }],
    "no-tabs": "off",
    "prefer-destructuring": "off",
    "indent": [2, "tab", {}],
    "no-unused-vars": "warn",
    "no-useless-return": "warn"
  }
}
