{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": ["airbnb/base"],
  "rules": {
      "semi": "warn",
      "no-plusplus": "off",
      "eol-last":"off",
      "max-len": ["error", 150, {"ignoreUrls": true}],
      "no-tabs": "off",
      "prefer-destructuring": "off",
      "indent":[2, "tab", {}],
      "no-unused-vars":"warn"
  }
}